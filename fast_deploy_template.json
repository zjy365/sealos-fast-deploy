      "name": "appsmith"
    },
    "spec": {
      "title": "Appsmith",
      "url": "https://www.appsmith.com/",
      "github": "https://github.com/appsmithorg/appsmith",
      "author": "sealos",
      "description": "Appsmith is an open-source platform to build, deploy, and maintain internal apps. You can build anything from simple CRUD apps, admin panels, dashboards to custom business apps and complicated multi-step workflows.",
      "readme": "https://raw.githubusercontent.com/appsmithorg/appsmith/release/README.md",
      "icon": "https://avatars.githubusercontent.com/u/67620218?s=96&v=4",
      "template_type": "inline",
      "defaults": {
        "app_host": {
          "type": "string",
          "value": "${{ random(8) }}"
        },
        "app_name": {
          "type": "string",
          "value": "appsmith-${{ random(8) }}"
        }
      },
      "inputs": null
    }
  },
