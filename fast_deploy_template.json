[
  {
    "apiVersion": "template.app.sealos.io/v1beta1",
    "kind": "Template",
    "metadata": {
      "name": "Halo"
    },
    "spec": {
      "title": "Halo",
      "url": "https://halo.run/",
      "github": "https://github.com/halo-dev/halo",
      "author": "sealos",
      "description": "Powerful and easy-to-use open source website building tool",
      "readme": "https://raw.githubusercontent.com/halo-dev/halo/main/README.md",
      "icon": "https://avatars.githubusercontent.com/u/48195280?s=48&v=4",
      "template_type": "inline",
      "defaults": {
        "app_host": {
          "type": "string",
          "value": "${{ random(8) }}"
        },
        "app_name": {
          "type": "string",
          "value": "halo-${{ random(8) }}"
        }
      },
      "inputs": {
        "admin_username": {
          "description": "Halo Super Admin Username",
          "type": "string",
          "default": "admin",
          "required": false
        },
        "admin_password": {
          "description": "Halo Super Admin Password",
          "type": "string",
          "default": "admin",
          "required": false
        }
      }
    }
  },
